    "browserify": "^16.0.0",
    "chai": "^4.0.2",
    "electron-typescript-definitions": "^1.0.0",
    "karma": "^5.0.9",
    "karma-browserify": "^5.1.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.0.0",
    "rimraf": "^2.5.4",
    "watchify": "^3.8.0"
  },
  "dependencies": {
    "make-dir": "^1.0.0",
